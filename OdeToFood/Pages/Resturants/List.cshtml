@page
@model OdeToFood.Pages.Resturants.ListModel
@{
}

<div>
    <h1>Resturants !</h1>
    <h2>@Model.Message</h2>
</div>
<div>
    <table class="table">
        @foreach (var resturant in Model.ResturantsList)c
        {
        <tr>
            <td>@resturant.Name</td>
            <td>@resturant.Location</td>
            <td>@resturant.Cusine</td>
        </tr>
            
        }
    </table>
</div>
